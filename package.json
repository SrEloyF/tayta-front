    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.511.0",
    "next": "14.1.4",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
