{
  "name": "taytaservice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "docker:build": "docker build --no-cache -t taytaservice .",
    "docker:run": "docker run -p 3000:3000 --env-file .env.production taytaservice",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down --remove-orphans",
    "docker:logs": "docker-compose logs -f",
    "docker:prune": "docker system prune -f",
    "clean-rebuild": "npm run docker:down && npm run docker:prune && npm run docker:build && npm run docker:up"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
